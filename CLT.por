programa {
  funcao inicio() {
    inteiro id, qntHoras 
    real valorHora, salario
    escreva("Id: ")
    leia(id)
    escreva("Qtd horas trabalhadas: ")
    leia(qntHoras)
    escreva("Digite o valor por horas: R$")
    leia(valorHora)
    salario = qntHoras * valorHora
    escreva("id: "+ id + "\nsalario: R$" + salario)


    
    
  }
}
